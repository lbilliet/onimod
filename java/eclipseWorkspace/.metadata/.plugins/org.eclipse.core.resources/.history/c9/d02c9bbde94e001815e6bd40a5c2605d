package tutoUdemyFr;

public class HelloUniverse {
	
	public static void main(String... args){
        int nbPlanetes=9;
        String strg1 = "Aux dernières nouvelles, le nombre total de planètes dans le système solaire est de : ";
		System.out.println(strg1);
        System.out.println(nbPlanetes);
        strg1 = "Il y a quelques années cependant, elles étaient au nombre de : ";
        nbPlanetes=8;
        System.out.println(strg1);
        System.out.println(nbPlanetes);
	}
}
	
	
        

